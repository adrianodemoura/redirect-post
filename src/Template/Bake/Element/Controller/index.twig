    {
        ${{ name }}Form = new {{ name }}Form();

        if ( $this->request->isPost() )
        {
            $data = $this->request->getData();
            if ( ${{ name }}Form->execute( $data ) )
            {
                $this->Redirect->saveRedirect( ['action'=>'visualizar'], $data);
