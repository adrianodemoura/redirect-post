{
    "name": "adrianodemoura/redirect-post",
    "description": "Componente redirectPost para cakePHP 3",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "redirect", "post", "request", "data"],
    "homepage": "https://github.com/adrianodemoura/redirect-post",
    "license": "MIT",
    "minimum-stability": "stable",
 
    "authors": [
        {
            "name": "Adriano Moura",
            "homepage": "https://github.com/adrianodemoura"
        }
    ],
 
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "^3.5"
    },

    "require-dev": {
        "cakephp/debug_kit": "^3.2.*"
    },
 
    "support": {
        "issues": "https://github.com/adrianodemoura/redirect-post/issues",
        "forum": "http://stackoverflow.com/tags/redirect-post",
        "source": "https://github.com/adrianodemoura/redirect-post"
    },
 
    "autoload": {
        "psr-4": {
            "RedirectPost\\": "src",
            "RedirectPost\\Test\\Fixture\\": "tests/Fixture/"
        }
    },
 
    "autoload-dev": {
        "psr-4": {
            "RedirectPost\\Test\\": "tests",
            "Cake\\Test\\" : "vendor/cakephp/cakephp/test"
        }
    }
}
